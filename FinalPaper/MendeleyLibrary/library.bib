issn = {1471-0056},
journal = {Nature Reviews Genetics},
number = {9},
pages = {672--672},
pmid = {22805710},
title = {{Genetics of osteoporosis from genome-wide association studies: advances and challenges}},
volume = {13},
year = {2012}
