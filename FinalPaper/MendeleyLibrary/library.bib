isbn = {1537-6605 (Electronic) 0002-9297 (Linking)},
issn = {15376605},
journal = {American Journal of Human Genetics},
keywords = {gene expression,genetic architecture,genetic association study,heritability,linear mixed models},
number = {2},
pages = {228--237},
pmid = {28065468},
publisher = {ElsevierCompany.},
title = {{The Genetic Architecture of Gene Expression in Peripheral Blood}},
